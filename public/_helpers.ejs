<%
var DOWs = ["Sunday", "Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday"]
var months = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'];

dateToReadable = function(d){
  var date = d.getUTCDate()

  var suffixes = {1: 'st', 2: 'nd', 3: 'rd'}
  var suffix = (date > 3 && date < 21)
    ? 'th'
    : suffixes[parseFloat(date.toString().slice(-1)[0])] || 'th'

  return DOWs[d.getUTCDay()]+" "
  +months[d.getUTCMonth()]+" "
  +date
  +suffix
  +", "+(1900+d.getYear()) + " 7PM"
}

pathToUrl = function(p){
  if(!p)return "";
  return p[0]=='/'?'https://5c01b9f7f6d5ea39c04ea7ba--jsla.netlify.com/'+p:p
}
%>
